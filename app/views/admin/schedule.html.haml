%h1= course.name
.tabbable
  %ul.nav.nav-tabs{:'data-tabs' => 'tabs'}
    %li.active= link_to 'Schedule', '#schedule', :'data-toggle' => 'tab'

  .tab-content
    #schedule.tab-pane.active
      #cal_container
      #new_content.hide

%script= render partial: 'cal.js', locals: { course: course, json: [] }
